package jurnal.no.pkg2;

/**
 *
 * @author Asisten
 */
public class hitung {
   public static int hsl(int nilai)
    {
        int i,tot=1;
        for (i=1;i<=nilai;i++)
        {
            tot=tot*i;
        }
        System.out.println("Faktorial dari "+nilai+"! : ");
        return(tot);
    } 
}
